" src=\"../assets/img/logo.png\"\n             alt=\"\"/>\n        <div class=\"login-form-text\">Xin电影 - 注册</div>\n      </div>\n      <div style=\"color: #91949c;font-weight: bolder\">\n        <p>Username</p>\n        <el-input class=\"login-form-input\" v-model=\"username\" placeholder=\"账 号\"></el-input>\n        <p>Password</p>\n        <el-input class=\"login-form-input\" placeholder=\"密 码\" v-model=\"password\" show-password></el-input>\n        <p>Check Password</p>\n        <el-input class=\"login-form-input\" placeholder=\"确 认 密 码\" v-model=\"checkPassword\"\n                  show-password></el-input>\n        <p>Email</p>\n        <el-input class=\"login-form-input\" v-model=\"email\" placeholder=\"邮 箱\"></el-input>\n        <p>Verification Code</p>\n        <el-input class=\"login-form-input\" v-model=\"verificationCode\" placeholder=\"验证码\"></el-input>\n        <el-button @click=\"sendEmail\" class=\"login-form-button\" type=\"primary\" :disabled=\"isSendDisabled\">{{ sendBtnText }}</el-button>\n        <div style=\"padding-top: 10px\">\n          <el-checkbox v-model=\"agree\">我已同意\"Xin电影\"用户注册协议!</el-checkbox>\n        </div>\n        <el-button @click=\"postLogin\" class=\"login-form-button\" type=\"primary\">SIGN UP</el-button>\n      </div>\n      <div class=\"login-form-footer\">\n        <el-link href=\"/login\" style=\"font-weight: bolder;font-size: 16px;color: #91949c;\"\n                 :underline=\"false\">\n          我有账号 去登录\n          <i style=\"font-weight: bolder;font-size: 15px\" class=\"el-icon-right\"></i>\n        </el-link>\n      </div>\n    </div>\n  </div>\n</template>\n\n<script>\n\nimport {Register} from \"@/api/user\"\nimport axios from \"axios\";\n\nexport default {\n  data() {\n    return {\n      username: '',\n      password: '',\n      checkPassword: '',\n      email: '',\n      verificationCode: '',\n      agree: false,\n      sendBtnText: '发送验证码',\n      isSendDisabled: false,\n      sendIntervalId: null,\n      sendTime: 60,\n    }\n  },\n  methods: {\n    postLogin() {\n      if (this.username.length < 6) {\n        this.$message({\n          message: '请输入不少于6位的用户名',\n          type: 'warning'\n        });\n        return\n      }\n      if (this.agree === false) {\n        this.$message({\n          message: '请勾选 我已同意\"Xin电影\"用户注册协议!',\n          type: 'warning'\n        });\n        return\n      }\n      if (this.password !== this.checkPassword) {\n        this.$message({\n          message: '您两次输入的密码不同!',\n          type: 'warning'\n        });\n        return\n      }\n      if (this.verificationCode === '') {\n        this.$message({\n          message: '请输入验证码',\n          type: 'warning'\n        });\n        return\n      }\n      const LoginData = {\n        username: this.username,\n        password: this.password,\n        email: this.email,\n        verificationCode: this.verificationCode,\n      };\n      console.log(LoginData);\n      Register(LoginData).then(res => {\n        if (res.success) {\n          this.$router.push(\"/login\")\n        }\n      })\n    },\n    sendEmail() {\n      if (this.email === '') {\n        this.$message({\n          message: '请输入您的邮箱',\n          type: 'warning'\n        });\n        return\n      }\n      const params = {\n        email: this.email\n      }\n      this.isSendDisabled = true;\n      this.sendBtnText = this.sendTime + 's后重发';\n      this.sendIntervalId = setInterval(() => {\n        if (this.sendTime > 0) {\n          this.sendTime -= 1;\n          this.sendBtnText = this.sendTime + 's后重发';\n        } else {\n          clearInterval(this.sendIntervalId);\n          this.sendTime = 60;\n          this.sendBtnText = '发送验证码';\n          this.isSendDisabled = false;\n        }\n      }, 1000);\n\n      // 发送验证码的代码开始\n      axios.post('/api/user/sendVerificationCode', params).then(response => {\n        console.log(response.data);\n        if (response.data.success) {\n          this.$message({\n            message: '验证码已发送，请留意邮箱',\n            type: 'success'\n          });\n        } else {\n          this.$message({\n            message: '验证码发送失败，请稍后重试',\n            type: 'error'\n          });\n        }\n      }).catch(error => {\n        console.log(error);\n        this.$message({\n          message: '验证码发送失败，请稍后重试',\n          type: 'error'\n        });\n      });\n    }\n  }\n}\n</script>\n\n\n<style scoped>\n\n.login {\n  width: 100%;\n  height: 100%;\n  background: #FFFFFF;\n}\n\n.login-form {\n  width: 500px;\n  position: absolute;\n  left: 50%;\n  top: 50%;\n  transform: translate(-50%, -50%);\n  letter-spacing: 2px;\n}\n\n.login-form-header {\n  height: 20px;\n  padding-left: 45px;\n  padding-bottom: 100px;\n}\n\n.login-form-text {\n  color: #000000;\n  font-weight: bold;\n  font-size: 30px;\n  padding-top: 15px;\n}\n\n.login-form-input {\n  margin-bottom: 10px;\n}\n\n.login-form-button {\n  border-radius: 3px;\n  width: 100%;\n  font-weight: 600;\n  font-size: 15px;\n  letter-spacing: 2px;\n  height: 60px;\n  background: #5a84fd;\n  box-shadow: 0 5px 30px rgb(0 66 8.5%);\n  margin-top: 35px;\n}\n\n.login-form-footer {\n  font-weight: bolder;\n  color: #91949c;\n  padding-top: 40px;\n  text-align: center;\n}\n\n>>> .el-input__inner {\n  height: 48px;\n}\n\n.el-checkbox {\n  color: #91949c;\n  font-weight: bolder;\n  font-size: 15px;\n}\n\n</style>\n"]}]}